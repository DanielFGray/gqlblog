{
  "name": "react-starter",
  "version": "0.1.0",
  "main": "index.js",
  "author": "DanielFGray <DanielFGray@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "nodemon -w webpack.config.babel.js -w .eslintrc.js -w .babelrc -x 'webpack-dev-server --progress'",
    "build": "NODE_ENV=production webpack --progress -p",
    "lint": "concurrently 'flow --quiet --color=always' 'eslint --ext=.js,.jsx src' 'stylelint src/**/*.{css,sss}' --prefix '{name}:' --names 'flow,eslint,stylelint' -c 'gray.reset'  && jest --coverage || true",
    "test": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.([sc]ss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "normalize.css": "^6.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "empty": "^0.10.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.44.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "prop-types": "^15.5.8",
    "react-test-renderer": "^15.5.4",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "sugarss": "^0.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
